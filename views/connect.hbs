<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>This Is Us | 47th-Cafe</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    {{!-- icon  --}}
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
    <link rel="shortcut icon" href="/static/images/logo.jpg" type="image/x-icon">
    
    {{!-- css  --}}
  <link rel="stylesheet" href="/static/css/style.css">

</head>

<body>
 
 {{!-- section 1  --}}
  {{> navbar data=navdetails}}

  {{!-- section 2  --}}
  <section class="menu-sec">
    <div class="container py-5">
        {{#each connectinfos}}
      <div class="row">
        <div class="col-md-12">
          <h1 class="text-center text-black menu-head">{{this.title}}</h1>
          <p class="text-center py-3 text-secondary">{{this.text1}} <br>{{this.text2}}<br>{{this.text3}}</p>
          </p>
        </div>
      </div>
{{/each}}
    </div>
  </section>

{{!-- section 3 contact form  --}}

<section class="form-page">
  <div class="container py-5">
    <div class="row">

<div class="col-md-5 py-5">
{{!-- left side  --}}
<form action="/process-contact-us-form" method="post">
<h1 class="text-center cont-us text-black">Contact Us</h1>
  <div class="mb-3">
    <div class="row">
      <div class="col-md-6 py-2">
    <label for="inputText1" class="form-label text-secondary">First Name*</label>
     <input type="text" class="form-control" name="firstName" id="fname" required>
      </div>
      <div class="col-md-6 py-2">
        <label for="inputText2" class="form-label text-secondary">Last Name*</label>
     <input type="text" class="form-control" name="lastName" id="lname" required>
      </div>
    </div>
    <label for="exampleInputEmail1" class="form-label py-2 text-secondary">Email address*</label>
    <input type="email" name="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" required>
  </div>

    <label for="exampleInputEmail1" class="form-label text-secondary">Message</label>
  <div class="form-floating py-2">
  <textarea name="message" class="form-control" placeholder="Leave a comment here" id="floatingTextarea2" style="height: 100px"></textarea>
  <label for="floatingTextarea2"></label>
</div>
  <button type="submit" class="btn btn-secondary" style="width: 100%;">Send</button>
</form>
</div>
<div class="col-md-2">
  <div class="d-flex" style="height: 500px;">
  <div class="vr"></div>
</div>
</div>
<div class="col-md-5 py-5 address-sec">
{{!-- right side  --}}
{{#each contactinfo2s}}
<div class="row">
  <div class="col-md-4 py-1">
    <i class="{{this.addressIcon}}"></i>
<p class="information">{{this.address}}</p>
<p class="text-secondary">{{this.addressText}}</p>
<i class="{{this.emailIcon}}"></i>
<p class="information">{{this.email}}</p>
<p class="text-secondary"><a href="{{this.emailUrl}}">{{this.emailText}}</a></p>
  </div>
<div class="col-md-3"></div>
  <div class="col-md-4 py-1">
    <i class="{{this.phoneIcon}}"></i>
<p class="information">{{this.phone}}</p>
<p class="text-secondary">{{this.phoneNum}}</p>
<div class="py-5">
<i class="{{this.followIcon}}"></i>
<p class="information">{{this.follow}}</p>
  <i class="{{this.facebook}}"></i>
  <i class="{{this.twitter}}"></i>
  <i class="{{this.instagram}}"></i>
  </div>
</div>
</div>
</div>
{{/each}}
    </div>
  </div>
</section>
  
  {{!-- map section  --}}
<section class="map-section">
  <div class="container-fluid">
    <div class="row">
      <div class="col-md-12">
        <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d65247916.04212203!2d139.36681395470166!3d-3.4905201479285974!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x80859a6d00690021%3A0x4a501367f076adff!2sSan%20Francisco%2C%20CA%2C%20USA!5e0!3m2!1sen!2sin!4v1712133368730!5m2!1sen!2sin" width="100%" height="450" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
      </div>
    </div>
  </div>
</section>

 {{!-- footers  --}}

{{> footer}}



  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
    crossorigin="anonymous"></script>
</body>

</html>