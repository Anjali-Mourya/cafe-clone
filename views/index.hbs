<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Wix Com Cafe</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    {{!-- icon  --}}
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
    <link rel="shortcut icon" href="/static/images/logo.jpg" type="image/x-icon">
 
 {{!-- style css  --}}
 <link rel="stylesheet" href="/static/css/style.css">
 {{!-- swiper css  --}} 
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css" />

  </head>
  <body>
 
 {{!-- navbar  --}}
    {{> navbar data=navdetails}}

{{!-- banner1 section  --}}

<section class="sec_section">
<div class="container-fluid">
  <div class="row">
{{#each bannerOnes}}
<div class="col-md-6 py-5 res_title text-center">

<p class="cafe_p">{{this.text}}</p>
<h1 class="text-white spot">{{this.titleOne}}</h1>
<h1 class="text-white spot">{{this.titleTwo}}</h1>

<a href="{{this.linkOneUrl}}"><button class="btn btn-outline-secondary mt-5 py-3 ">{{this.linkTextOne}}</button></a>
<a href="{{this.linkTwoUrl}}"><button class="btn btn-outline-secondary mt-5 py-3">{{this.linkTextTwo}}</button></a>
</div>
{{/each}}


  </div>

</div>
</section>
{{!-- slider swiper  --}}

<section class="slider_container">
  <div class="container-fluid bg-secondary py-3">
    <div class="row">
<div class="col-md-12">

<div class="swiper card_slider">

<div class="swiper-wrapper">
{{#each swipers}}
<div class="swiper-slide">
  <div class="title-box">
    <p class="text-center text-white">{{this.textOne}}</p>
  </div>
</div>

<div class="swiper-slide ">
  <div class="title-box">
    <p class="text-center text-white">{{this.textTwo}}</p>
  </div>
</div>
{{/each}}

  </div>
  <div class="swiper-button-next text-white "></div>
    <div class="swiper-button-prev text-white " ></div>

</div>

</div>

</div>

    </div>
  </div>
</section>

{{!-- banner 2 section  --}}

<section class="mysection">

<div class="container">
<div class="row">
  {{#each bannerTwos}}
<div class="col-md-5 pt-5 title-eat">
  {{!-- left side --}}
  <h1 class="eat">{{this.heading}}</h1>
  <p class="break-title">{{this.title}}</p>
  <p class="text-secondary">{{this.subTitle}}</p>

  <a href="{{this.linkUrl}}"><button class"btn-outline-secondary mybtn">{{this.linkText}}</button></a>
</div>
<div class="col-md-2"></div>
<div class="col-md-4 text-center">
  {{!-- right side  --}}
  <img src="{{this.image}}" class="food-img" alt="">
</div>
{{/each}}
</div>

</div>

</section>

{{!-- banner3 section  --}}
<section class="mysection">

<div class="container-fluid">
<div class="row">
  {{#each bannerThrees}}
  <div class="col-md-6">
  {{!-- left side  --}}
  <img src="{{this.image}}" class="food-img" alt="">
</div>
<div class="col-md-2"></div>
<div class="col-md-4 pt-5 title-eat">
  {{!-- right side --}}
  <h1 class="eat">{{this.heading}}</h1>
  <p class="break-title">{{this.title}}</p>
  <p class="text-secondary">{{this.subTitle}}</p>

  <a href="{{this.linkUrl}}"><button class"btn-outline-secondary mybtn">{{this.linkText}}</button></a>
</div>

{{/each}}
</div>

</div>

</section>

{{!-- banner4 section  --}}

<section class="mysection">

<div class="container">
<div class="row">
  {{#each bannerFours}}
<div class="col-md-5 pt-5 title-eat">
  {{!-- left side --}}
  <h1 class="eat">{{this.heading}}</h1>
  <p class="break-title">{{this.title}}</p>
  <p class="text-secondary">{{this.subTitle}}</p>

  <a href="{{this.linkUrl}}"><button class"btn-outline-secondary mybtn">{{this.linkText}}</button></a>
</div>
<div class="col-md-2"></div>
<div class="col-md-4 text-center">
  {{!-- right side  --}}
  <img src="{{this.image}}"class="enjoy-img" alt="">
</div>
{{/each}}
</div>

</div>

</section>

{{!--banner 5 section   --}}

<section id="address">
<div class="container-fluid py-5 drop-sec">
  <div class="row">
    {{#each bannerFives}}

<h1 class="eat text-center">{{this.heading}}</h1>
<div class="col-md-5 text-center left-sec py-4">
  {{!-- left side  --}}
<p class="ban-title">{{this.titleOne}}</p>
<p class="text-secondary">{{this.subTitleOne}}<br> {{this.subTitleOne2}}</p>
</div>

<div class="col-md-1 py-4">
  <div class="d-flex" style="height: 150px;">
  <div class="vr"></div>
</div>

</div>
<div class="col-md-5 text-center right-sec py-4">
  {{!-- right side  --}}
  <p class="ban-title">{{this.titleTwo}}</p>
  <p class="text-secondary">{{this.subTitleTwo}}<br>{{this.subTitleTwo2}}<br>{{subTitleTwo3}}</p>
</div>
{{/each}}
  </div>
</div>

</section>

{{!-- banner 6 section  --}}
<section class="text-center">
<div class="container-fluid resturant-img">
  <div class="row">
    <div class="col-md-12">

<div class="flex">
  <div class="box">
  <img src="/static/images/img1.jpg" alt="">
  <img src="/static/images/img2.jpg" alt="">
  <img src="/static/images/img3.jpg" alt="">
  <img src="/static/images/img4.jpg" alt="">
  <img src="/static/images/img5.jpg" alt="">
</div>
</div>

    </div>
  </div>
</div>
</section>

{{!-- footer  --}}
{{> footer footers =footers}}


    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
    {{!-- swiper js  --}}
      <script src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js"></script>
      {{!-- js  --}}
      <script src="/static/js/swiper.js"></script>
  </body>
</html>