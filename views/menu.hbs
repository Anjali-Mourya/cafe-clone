<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>This Is Us | 47th-Cafe</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
  <link rel="stylesheet" href="/static/css/style.css">
    <link rel="shortcut icon" href="/static/images/logo.jpg" type="image/x-icon">


</head>

<body>
 
 {{!-- section 1  --}}
  {{> navbar data=navdetails}}

  {{!-- section 2  --}}
  <section class="menu-sec">
    <div class="container py-5">
      {{#each menu1s}}
      <div class="row">
        <div class="col-md-12">
          <h1 class="text-center text-black menu-head">{{this.title1}}</h1>
          <p class="text-center py-3 text-secondary">{{this.subTitle1}} <br>
            {{this.subTitle2}} <br>
            {{this.subTitle3}}<br> 
            {{this.subTitle4}}

          </p>
        </div>
      </div>
      {{/each}}
    </div>
  </section>

  {{!-- section3  --}}

  <section class="bakery-sec">
    {{#each menuitems}}
    <div class="container text-center">
      <h2 class="text-black">{{this.heading}}</h2>
  
      <div class="row text-center">
  
        <h4 class="py-5 text-success">{{this.title}}</h4>
        <div class="col-md-3 ">
          <h5 class="text-black">{{this.subTitle1}}</h5>
          <p>{{this.text1}}<br> {{this.text2}}</p>
          <p class="rs">{{this.price1}}</p>
        </div>
        <div class="col-md-1"></div>
        <div class="col-md-3">
          <h5 class="text-black">{{this.subTitle2}}</h5>
          <p>{{this.text3}}<br>
          {{this.text4}}</p>
          <p class="rs">{{this.price2}}</p>
        </div>
        <div class="col-md-1"></div>
        <div class="col-md-3">
          <h5 class="text-black">{{this.subTitle3}}</h5>
          <p>{{this.text5}}<br>
             {{this.text6}}<br>
              {{this.text7}}</p>
          <p class="rs">{{this.price3}}</p>
        </div>
        <div class="col-md-1"></div>

      </div>
    </div>
    {{/each}}
  </section>

 {{!-- footers  --}}

{{> footer}}



  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
    crossorigin="anonymous"></script>
</body>

</html>